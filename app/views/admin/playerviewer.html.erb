
<%= render 'admin/adminnav' %>
<div class="table-responsive">
<table class = "table table-condensed table-hover sortable">
  <tr >
    <th>Player</th>
    <%= render 'admin/culstatsth' %>
    <th> R/G </th>

  </tr>
  <% @Games.each do |game| %>
  <tr>
    <td><%= link_to  game.firstname + ' ' + game.lastname, search_path(:secretkey =>game.secretkey) %></td>
    <td><%= game.atbat %></td>
    <td><%= game.hit %></td>
    <td><%= game.run %></td>
    <td><%= game.rbi %></td>
    <td><%= game.totalbases %></td>
    <td class="success"><%= game.stolenbase %></td>
    <td class="success"><%= game.single %></td>
    <td class="success"><%= game.double %></td>
    <td class="success"><%= game.triple %></td>
    <td class="success"><%= game.homerun %></td>
    <td class="warning"><%= game.strikeout %></td>
    <td class="warning"><%= game.error %></td>
    <td class="danger"><%= game.sacfly %></td>
    <td class="danger"><%= game.lineout %></td>
    <td class="danger"><%= game.groundout %></td>
    <td class="danger"><%= game.flyout %></td>
    <td class="danger"><%= game.fc %></td>
    <td class="warning"><%= game.fss%></td>
    <td class="warning"><%= game.lgf%></td>
    <td><%= game.avg %></td>
    <td><%= game.obp %></td>
    <td><%= game.slg%></td>
    <td><%= game.ops%></td>
    <td class ="info"><%= number_with_precision((17.11* game.obp + 11.13*game.slg - 5.66)/6, precision:2) %></td>

  </tr>
  <% end %>
</table>
</div>
